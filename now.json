{
  "name": "chronogram",
  "alias": "chronogram.now.sh",
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node@canary"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/endpoints/$1.ts"
    }
  ]
}
